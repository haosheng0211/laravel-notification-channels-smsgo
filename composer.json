{
    "name": "laravel-notification-channels/smsgo",
    "description": "Laravel SMSGo Notification Channel",
    "keywords": [
        "Laravel",
        "SMSGo",
        "notification channel",
        "SMS notifications",
        "communication",
        "messaging",
        "notifications"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mr.Jin",
            "email": "haosheng0211@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "giggsey/libphonenumber-for-php": "^8.13",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SMSGo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SMSGo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\SMSGo\\SMSGoServiceProvider"
            ]
        }
    }
}
